
@-webkit-keyframes showWindow{
from{height: 0;}
to{height: 80%;}
}
@keyframes showWindow{
from{height: 0;}
to{height: 80%;}
}
.popup{
	position: fixed;
	bottom: 0;
	left: 0;
	-webkit-animation-name: showWindow;
	        animation-name: showWindow;
	-webkit-animation-duration: .5s;
	        animation-duration: .5s;
	/* right: 0; */
	height: 80%;
	width: 100%;
	background-color: white;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	z-index: 101;
}

